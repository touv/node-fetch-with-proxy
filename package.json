    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "cross-fetch": "^3.0.5",
